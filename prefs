# Note: this color-scheme only really works if your terminal is 
# on the solarized colorscheme.  Tested in windows terminal.

source color-schemes/solarized.sh


CS_PLAIN="$COL_BG_BASE02$COL_FG_BASE1"

CS_DIR_RW="$COL_FG_BLUE"
CS_DIR_RO="$COL_FG_RED"

CS_GIT_OK="$CS_PLAIN"
CS_GIT_MOD="$COL_FG_RED"
CS_GIT_IDX="$COL_FG_GREEN"
